<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.itsoft.ab.persistence.FilialsMapper">
        <select id="getFilials" resultType="FilialModel">
                SELECT
                  f.id,
                  f.name,
                  f.address
                FROM filials f
        </select>
</mapper>
