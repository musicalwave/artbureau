<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.itsoft.ab.persistence.JCallsMapper">

    <insert id="insert" parameterType="JCallsModel">
        INSERT INTO J_CALLS (callId, action, userId)
        VALUES (#{callId}, #{action}, #{userId})
    </insert>

</mapper>
